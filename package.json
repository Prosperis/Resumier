{
  "name": "resumier",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "bun@1.3.0",
  "scripts": {
    "dev": "vite",
    "build": "bunx tsr generate && tsc -b && vite build",
    "build:analyze": "bunx tsr generate && tsc -b && vite build && bunx vite-bundle-visualizer",
    "preview": "vite preview",
    "optimize:images": "node optimize-images.mjs",
    "generate:pwa-icons": "node generate-pwa-icons.mjs",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:accessibility": "npx vitest run src/test/accessibility",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "oxlint src/",
    "lint:check": "oxlint src/",
    "lint:fix": "oxlint --fix src/",
    "format": "oxfmt src/",
    "format:check": "oxfmt --check src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "routes:generate": "bunx tsr generate",
    "routes:watch": "bunx tsr watch",
    "deps:check": "taze",
    "deps:update": "taze major -w",
    "deps:update:minor": "taze minor -w",
    "deps:update:patch": "taze patch -w",
    "deps:interactive": "taze -I",
    "prepare": "husky"
