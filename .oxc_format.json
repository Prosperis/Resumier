{
  "$schema": "https://docs.rs/oxc/latest/oxc/formatter/struct.FormatterOptions.html",
  "indentWidth": 2,
  "useTabs": false,
  "lineWidth": 100,
  "indentStyle": "space",
  "quotes": "double",
  "jsxQuotes": "double",
  "trailingComma": "es5",
  "semicolons": "always",
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false
}
